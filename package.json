{
  "name": "API-Supply",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "setupFiles": [
      "./__test__/lib/jest-setup.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon index.js",
    "start:debug": "DEBUG=APISupply* nodemon index.js",
    "start-db": "mkdir -p ./db && mongod --dbpath ./db",
    "stop-db": "killall mongod",
    "test": "jest --runInBand",
    "test:exit": "jest --forceExit",
    "test:watch": "jest --watchAll",
    "lint": "eslint .",
    "lint:test": "npm run lint && npm test"
  },
  "keywords": [
    "images",
    "photos",
    "uploads",
    "codez"
  ],
  "author": "lonewalker72",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.5.0",
    "faker": "^4.1.0",
    "jest": "^20.0.4",
    "superagent": "^3.6.0",
    "browser-sync": "2.18.13",
    "gulp": "^3.9.1"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "jsonwebtoken": "^8.0.0",
    "mongoose": "^4.11.10",
    "bootstrap": "^4.0.0-beta",
    "jquery": "^3.2.1",
    "jquery.easing": "^1.4.1",
    "popper.js": "^1.11.1"
  }
}
